    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "microbundle": "^0.8.3",
    "mini-css-extract-plugin": "^0.5.0",
    "minimatch": "^3.0.4",
    "mocha": "^3.0.2",
    "module-alias": "^2.1.0",
